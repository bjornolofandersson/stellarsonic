---
import { Entity } from '@lib/interfaces';
import Hero from './hero.svelte';

interface Props {
  entity: Entity<any>;

  section: number;

  page: Entity<any>;

  navigation: any;
}

const { entity, section, page, navigation } = Astro.props;
---

<Hero data={entity} section={section} page={page} navigation={navigation} client:load />
